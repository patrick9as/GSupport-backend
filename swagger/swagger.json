{
    "openapi": "3.0.0",
    "info": {
      "title": "Minha API",
      "version": "1.0.0"
    },
    "paths": {
      "/atendimentos": {
        "get": {
          "summary": "Retorna uma lista de atendimentos",
          "responses": {
            "200": {
              "description": "OK"
            }
          }
        },
        "post": {
          "summary": "Cria um novo atendimento",
          "requestBody": {
            "description": "Dados do usuário",
            "required": true,
            "content": {
              "multipart/form-data:": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "image":{
                        "type": "string",
                        "format": "binary"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Criado"
            },
            "400": {
              "description": "Requisição inválida"
            }
          }
        }
      }
    }
  }
  